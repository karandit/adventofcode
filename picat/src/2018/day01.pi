findSeen(Nrs, Len, Seen, Sum, I, Found), Seen.has_key(Sum) => Found = Sum.
findSeen(Nrs, Len, Seen, Sum, I, Found), Seen.put(Sum) =>
    findSeen(Nrs, Len, Seen, Sum + Nrs[I], cond(I == Len, 1, I + 1), Found).

main([File]) =>
    Inputs = read_file_lines(File).map(to_int).to_array(),
    findSeen(Inputs, Inputs.len, new_set(0), 0, 1, Part2),
    printf("Part1: %w; Part2: %w%n", Inputs.sum, Part2).
